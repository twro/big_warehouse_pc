}
.check-btn{
   position: relative;
}
.check-btn::after{
 background-color: #fff;
  top:10rpx;
  left:-10rpx;
  position: absolute;
  display: block;
  content: "";
  width: 20rpx;
  height: 20rpx;
  border-top: 2rpx solid #dbdbdb;
  border-right: 2rpx solid #dbdbdb;
  transform: rotate(45deg);
}
